    <script>

        function verifyPassword() {
            let val1 = true
            let val2 = true
            let val3 = true
            let val4 = true


            const pw1 = document.getElementById("password").value;
            const pw2 = document.getElementById("password2").value;
            let x = document.getElementById("name").value;
            let y = document.getElementById("pass").value;
            var atposition = y.indexOf("@");
            var dotposition = y.lastIndexOf(".");

            if (x.length < 3) {
                alert("Name must be filled out");
                val1 = false
            }
            else if (atposition < 1 || dotposition < atposition + 2 || dotposition + 2 >= y.length) {
                alert("Please enter a valid e-mail address")
                val2 = false
            }
            else if (pw1 == "" || pw2 == "") {
                alert("enter both the feild")
                val3 = false
            }

            else if (pw2.length != 0 && pw2.length != pw1.length || pw1 != pw2) {
                alert("enter same passwords")
                val4 = false
            }

            if (val1 && val2 && val3 && val4) {
                return true
            } else {
                return false
            }
        }
    </script>


